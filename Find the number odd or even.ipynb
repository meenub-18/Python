   "source": [
    "#Odd or Even\n",
    "i = int(input(\"Please enter the number:\"))\n",
    "\n",
    "if (i % 2 == 0):\n",
    "    print(\"even\")\n",
    "else:\n",
    "    print(\"odd\")\n"
   ]
